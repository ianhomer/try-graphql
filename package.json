{
  "name": "try-graphql",
  "version": "1.0.0",
  "description": "Try GraphQL",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ianhomer/try-graphql.git"
  },
  "bugs": {
    "url": "https://github.com/ianhomer/try-graphql/issues"
  },
  "homepage": "https://github.com/ianhomer/try-graphql#readme",
  "author": "",
  "keywords": [],
  "main": "index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean": "rimraf build test",
    "eslint": "eslint src --ext .ts",
    "eslint:fix": "eslint src --ext .ts --fix",
    "lint": "npm run prettier && npm run eslint",
    "lint:fix": "npm run prettier:fix && npm run eslint:fix && npm run package:fix",
    "package:fix": "format-package -w",
    "prettier": "npx prettier --check .",
    "prettier:fix": "npx prettier --write .",
    "start": "ts-node ./src/server.ts | npx pino-pretty",
    "update": "npx npm-check-updates -u && npm install --package-lock-only --no-audit"
  },
  "scripts-info": {
    "fix": "Try to automatically fix any linting problems",
    "info": "Display information about the package scripts"
  },
  "dependencies": {
    "@types/express": "^4.17.13",
    "@types/express-graphql": "^0.9.0",
    "@types/graphql": "^14.5.0",
    "@types/pino": "^7.0.4",
    "express": "^4.17.1",
    "express-graphql": "^0.12.0",
    "graphql": "^16.0.1",
    "pino": "^7.5.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  },
  "devDependencies": {
    "@types/chai": "^4.2.22",
    "@types/mocha": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "chai": "^4.3.4",
    "eslint": "^8.4.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "format-package": "^6.1.0",
    "lodash": "^4.17.21",
    "minimist": ">=1.2.5",
    "mocha": "^9.2.2",
    "prettier": "^2.5.1"
  }
}
