{
  "name": "try-graphql",
  "version": "1.0.0",
  "description": "Try GraphQL",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ianhomer/try-graphql.git"
  },
  "bugs": {
    "url": "https://github.com/ianhomer/try-graphql/issues"
  },
  "homepage": "https://github.com/ianhomer/try-graphql#readme",
  "author": "",
  "keywords": [],
  "main": "index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean": "rimraf build test",
    "eslint": "eslint src --ext .ts",
    "eslint:fix": "eslint src --ext .ts --fix",
    "lint": "pnpm run prettier && pnpm run eslint",
    "lint:fix": "pnpm run prettier:fix && pnpm run eslint:fix && pnpm run package:fix",
    "package:fix": "format-package -w",
    "prettier": "npx prettier --check .",
    "prettier:fix": "npx prettier --write .",
    "start": "ts-node ./src/server.ts | npx pino-pretty",
    "update": "pnpm update -rL"
  },
  "scripts-info": {
    "fix": "Try to automatically fix any linting problems",
    "info": "Display information about the package scripts"
  },
  "dependencies": {
    "@types/express": "^4.17.15",
    "@types/express-graphql": "^0.9.0",
    "@types/graphql": "^14.5.0",
    "@types/pino": "^7.0.5",
    "express": "^4.18.2",
    "express-graphql": "^0.12.0",
    "graphql": "^16.6.0",
    "pino": "^7.11.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/mocha": "^9.1.1",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "chai": "^4.3.7",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "format-package": "^6.1.0",
    "lodash": "^4.17.21",
    "minimist": "^1.2.7",
    "mocha": "^9.2.2",
    "prettier": "^2.8.1",
    "rimraf": "^3.0.2"
  }
}
